<config>
    <tabs>
        <easy>
            <label><![CDATA[<img style="width:202px; height:23px;" alt="Logo Easygento" title="Easygento by Easycom Solutions" src="data:image/png;base64,"/>]]></label>
            <sort_order>110</sort_order>
        </easy>
    </tabs>
    
    <sections>
        <easy_multipledomains module="easy_multipledomains" translate="label">
            <label>Multiple Domains</label>
            <tab>easy</tab>
            <frontend_type>text</frontend_type>
            <sort_order>100</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <general module="easy_multipledomains" translate="label">
                    <label>Global Configuration</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>0</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <expanded>1</expanded>
                    <fields>
                        <number_of_domains module="easy_multipledomains" translate="label comment">
                            <label>Number of domains</label>
                            <backend_model>easy_multipledomains/config_general_numberOfDomains</backend_model>
                            <frontend_type>text</frontend_type>
                            <sort_order>0</sort_order>
                            <validate>validate-not-negative-number</validate>
                            <comment><![CDATA[Number of domains that magento will handle as the same way than Base_URL domain. <br/>Be carefull if you reduce the limit, all configuration over the new limit will be erase.]]></comment>
                            <show_in_default>1</show_in_default>
                        </number_of_domains>        
                    </fields>
                </general>
            </groups>
        </easy_multipledomains>
